<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>EndDateShouldBeGreaterThanStartDate</fullName>
    <active>true</active>
    <description>End Date Should Be Greater Than Start Date</description>
    <errorConditionFormula>AND (NOT(ISBLANK(End_Date__c)),
					End_Date__c &lt; Start_Date__c)</errorConditionFormula>
    <errorDisplayField>End_Date__c</errorDisplayField>
    <errorMessage>End Date should be greater than Start Date</errorMessage>
</ValidationRule>
